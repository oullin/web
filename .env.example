# --- The environment where the api is running under.
#     example: production, local, staging, etc.
VITE_API_ENV=

# --- The oullin.io/api/ URL.
#     https://github.com/oullin/api
VITE_API_URL=

# --- The oullin.io VPS/HOST URL.
#     https://github.com/
VITE_HOST_URL=

# --- The given application registered account name
VITE_ACCOUNT_NAME=

# --- The given application public key
VITE_PUBLIC_KEY=

# --- The api host directory
ENV_API_LOCAL_DIR=

# --- Public site URL used for canonical links (no trailing slash)
VITE_SITE_URL=http://localhost:5173

# --- Sentry Configuration (optional)
# --- Sentry DSN for error monitoring
VITE_SENTRY_DSN=

# --- Sentry release version (optional, defaults to web@VITE_APP_VERSION)
VITE_SENTRY_RELEASE=

# --- Sentry traces sample rate (0.0 to 1.0, default: 1.0)
VITE_SENTRY_TRACES_SAMPLE_RATE=

# --- Sentry replays session sample rate (0.0 to 1.0, default: 0.1)
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=

# --- Sentry replays on error sample rate (0.0 to 1.0, default: 1.0)
VITE_SENTRY_REPLAYS_ERROR_SAMPLE_RATE=
